{
  "name": "carbon-cli",
  "version": "1.0.0",
  "scripts": {
    "build": "yarn build:clean && yarn build:compile",
    "build:compile": "tsc",
    "build:clean": "rm -rf ./dist/"
  },
  "description": "Personal template for writing Node CLI projects in Typescript.",
  "repository": "nictar/carbon",
  "author": "Nico Dinata <nicodnt@icloud.com>",
  "license": "MIT",
  "keywords": [
    "cli",
    "template",
    "typescript",
    "nodejs"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.22.0",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-rn": "^1.1.6",
    "prettier": "2.2.1",
    "typescript": "^4.2.4"
  },
  "importSort": {
    ".js": {
      "parser": "babylon",
      "style": "rn"
    },
    ".ts": {
      "parser": "typescript",
      "style": "rn"
    }
  }
}
